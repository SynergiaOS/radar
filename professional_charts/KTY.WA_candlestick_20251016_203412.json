            "type": "function",
            "body": "\n                                function(context) {\n                                    const label = context.dataset.label || '';\n                                    if (label === 'Cena') {\n                                        const o = context.raw.o;\n                                        const h = context.raw.h;\n                                        const l = context.raw.l;\n                                        const c = context.raw.c;\n                                        return [\n                                            'O: ' + o.toFixed(2) + ' PLN',\n                                            'H: ' + h.toFixed(2) + ' PLN',\n                                            'L: ' + l.toFixed(2) + ' PLN',\n                                            'C: ' + c.toFixed(2) + ' PLN'\n                                        ];\n                                    }\n                                    return label + ': ' + context.parsed.y.toFixed(2) + ' PLN';\n                                }\n                                "
          }
        }
      }
    },
